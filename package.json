{
  "name": "webpack-react-wechat",
  "version": "1.0.0",
  "description": "Wechat h5 page based on webpack and react.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dll-dev": "webpack --config ./build/webpack.config.dll-dev.babel.js",
    "dll-prd": "webpack --config ./build/webpack.config.dll-prd.babel.js",
    "predev": "npm run clean && npm run dll-dev",
    "prebuild": "npm run clean && npm run dll-prd",
    "postbuild": "rimraf dist/vendors",
    "build": "webpack -p --env=prd",
    "dev": "webpack-dev-server --env=dev",
    "start": "node express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/webpack-react-wechat.git"
  },
  "keywords": [
    "webpack",
    "react",
    "pc",
    "ant-design",
    "site",
    "website"
  ],
  "author": "afei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afeiship/webpack-react-wechat/issues"
  },
  "homepage": "https://github.com/afeiship/webpack-react-wechat#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "http-post-config": "github:afeiship/http-post-config",
